                <div class="ons-js-adv-filter">
                  <div class="ons-grid ons-grid--column@xxs@s ons-u-mb-m@m">
                    <div class="ons-grid__col ons-col-4@m">
                      <button type="button" class="ons-btn ons-adv-filter__trigger ons-js-adv-filter__trigger ons-btn ons-btn--secondary ons-btn--small ons-u-mb-m" aria-label="Toggle filters" aria-expanded="false" aria-controls="filter-panel">
                        <span class="ons-btn__inner"><span class="ons-btn__text">Show filters</span>
                        </span>
                      </button>
                      <div class="ons-adv-filter__panel ons-js-adv-filter__panel" id="filter-panel">
                        <h2 class="ons-u-fs-l">Filters</h2>
                        <form class="ons-js-adv-filter__form" method="POST">
                          <button class="ons-adv-filter__reset ons-js-adv-filter__reset ons-btn ons-btn--secondary ons-btn--small" type="reset"><span class="ons-btn__inner">Reset all filters</span></button>
                         
                                      <div class="ons-adv-filter__item ons-js-adv-filter__item" data-default-text="All types" data-multi-select-text="{n} filters selected">
                            <fieldset class="ons-fieldset" aria-controls="adv-filter-gallery">
                              <legend class="ons-fieldset__legend">Categories</legend>
                              <div class="ons-adv-filter__selection">
                                <span class="ons-u-vh">Active filters: </span>
                                <span class="ons-u-fs-s ons-js-adv-filter__selection">All types</span>
                              </div>
                              <div class="ons-input-items">
                                <p class="ons-checkboxes__label"></p>
                                <div class="ons-checkboxes__items">
                                  <span class="ons-checkboxes__item">
                                    <span class="ons-checkbox ons-checkbox--toggle">
                                      <input type="checkbox" id="poster" class="ons-checkbox__input ons-js-checkbox" value="poster" data-filter="poster" checked>
                                      <label class=" ons-checkbox__label " for="poster" id="poster-label">Analytics and Surveys</label>
                                    </span>
                                  </span>
                                </div>
                              </div>
                            </fieldset>
                          </div>
                          
                          <div class="ons-adv-filter__actions">
                            <button type="button" class="ons-btn ons-btn ons-btn--small ons-js-adv-filter__show">
                              <span class="ons-btn__inner"><span class="ons-btn__text">Show (<span class="ons-js-adv-filter__show-results">7</span> results)</span>
                              </span>
                            </button>
                            <button type="button" class="ons-btn ons-btn--small ons-btn--secondary ons-js-adv-filter__close">
                              <span class="ons-btn__inner"><span class="ons-btn__text">Close</span>
                              </span>
                            </button>
                          </div>
                        </form>
                      </div>
                    </div>
                    <div class="ons-grid__col ons-col-8@m">
                      <div class="ons-adv-filter__results-options">
                        <div class="ons-adv-filter__results-count">
                          <span class="ons-js-adv-filter__results-count">9</span> results of 1
                        </div>
                        <div class="ons-adv-filter__results-sort">
                          <label class="ons-label" for="sort">Sort by</label>
                          <select class="ons-input ons-input--select" id="sort" name="sort" aria-controls="adv-filter-gallery" data-sort="true">
                            <option value="index" data-sort-number="true">Latest</option>
                            <option value="index" data-sort-order="desc" data-sort-number="true">Oldest</option>
                          </select>
                        </div>
                      </div>
                      <!--list -->
                      <ul id="adv-filter-gallery" class="ons-document-list ons-adv-filter__gallery ons-js-adv-filter__gallery" data-filter-animation="off">
                        <li class="ons-document-list__item ons-filter__item ons-js-filter__item" data-filter="poster english local-authorities community-groups recruitment" data-sort-index="1">
                         
